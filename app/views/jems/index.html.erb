<h1>Listing jems</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Seq</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jems.each do |jem| %>
      <tr>
        <td><%= jem.name %></td>
        <td><%= jem.seq %></td>
        <td><%= jem.comment %></td>
        <td><%= link_to 'Show', jem %></td>
        <td><%= link_to 'Edit', edit_jem_path(jem) %></td>
        <td><%= link_to 'Destroy', jem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jem', new_jem_path %>
